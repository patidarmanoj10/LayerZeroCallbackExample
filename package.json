      "solhint:fix": "solhint --fix 'contracts/**/*.sol'",
      "eslint": "eslint --cache ./test/**/*.ts",
      "eslint:fix": "eslint --cache --fix ./test --ext .ts",
      "lint": "npm run prettier && npm run solhint && npm run eslint",
      "clean": "hardhat clean",
