      "@types/lodash": "4.14.191",
      "@types/mocha": "9.1.1",
      "@types/node": "18.11.18",
      "@typescript-eslint/eslint-plugin": "5.57.1",
      "@typescript-eslint/parser": "5.57.1",
      "compare-versions": "4.1.4",
